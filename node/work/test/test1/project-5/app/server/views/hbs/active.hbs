<div class="active">
    <h3>Active Tunnels</h3>

    {{#if a2}}
        <ul>
            {{#each content}}
                <p>{{@index}}: {{this}}</p>
            {{else}}
                <p class="empty">No content</p>
            {{/each}}
        </ul>

        <h4>delete tunnel <[d+n]></h4>
    {{/if}}

    <h4>new tunnel <[c]></h4>

    <input type="text" style="width: 50px">
    <button>click</button>
</div>